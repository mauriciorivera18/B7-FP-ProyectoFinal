int calcularPasajeros(int cambios[][2], int x)
{
    int total;
    int abordo=0, bajas=0;
    int i,k;
    int x = 5;
 for (i=0; i<x; i++)
 {
    for (k=0; k<1; k++)
 {
        abordo= abordo+cambios[i][k];

}
    	 for (k=1; k<2; k++)
        	bajas= bajas+cambios[i][k];
}
 total= abordo-bajas;
 return total;
}

